@article{rogoff2017,
  title = {Noticing {{Learners}}' {{Strengths Through Cultural Research}}},
  author = {Rogoff, Barbara and Coppens, Andrew D. and Alcal{\'a}, Luc{\'i}a and {Aceves-Azuara}, Itzel and Ruvalcaba, Omar and L{\'o}pez, Ang{\'e}lica and Dayton, Andrew},
  year = {2017},
