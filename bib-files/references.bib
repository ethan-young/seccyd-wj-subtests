@article{ursache2016,
  title = {Neurocognitive Development in Socioeconomic Context: {{Multiple}} Mechanisms and Implications for Measuring Socioeconomic Status},
  shorttitle = {Neurocognitive Development in Socioeconomic Context},
  author = {Ursache, Alexandra and Noble, Kimberly G.},
