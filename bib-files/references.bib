  author = {Juster, Robert-Paul and McEwen, Bruce S. and Lupien, Sonia J.},
  year = {2010},
  month = sep,
  journal = {Neuroscience and Biobehavioral Reviews},
  volume = {35},
  number = {1},
  pages = {2--16},
  issn = {0149-7634},
  doi = {10/cp67zv},
  abstract = {The allostatic load model expands the stress-disease literature by proposing a temporal cascade of multi-systemic physiological dysregulations that contribute to disease trajectories. By incorporating an allostatic load index representing neuroendocrine, immune, metabolic, and cardiovascular system functioning, numerous studies have demonstrated greater prediction of morbidity and mortality over and beyond traditional detection methods employed in biomedical practice. This article reviews theoretical and empirical work using the allostatic load model vis-a-vis the effects of chronic stress on physical and mental health. Specific risk and protective factors associated with increased allostatic load are elucidated and policies for promoting successful aging are proposed. (C) 2009 Elsevier Ltd. All rights reserved.},
  langid = {english},
  keywords = {3rd national-health,Aging,Allostatic load,Biomedicine,Chronic stress,chronic-fatigue-syndrome,Cognition,cumulative biological risk,glucose   allostasis,Health,metabolic syndrome,nutrition examination survey,older population,physiological   dysregulation,Resilience,social relationships},
