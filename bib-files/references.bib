@article{szepsenwol2019,
  title = {The Effect of Early-Life Harshness and Unpredictability on Intimate Partner Violence in Adulthood: {{A}} Life History Perspective},
  shorttitle = {The Effect of Early-Life Harshness and Unpredictability on Intimate Partner Violence in Adulthood},
  author = {Szepsenwol, Ohad and Zamir, Osnat and Simpson, Jeffry A.},
  year = {2019},
  month = may,
  journal = {Journal of Social and Personal Relationships},
  volume = {36},
  number = {5},
  pages = {1542--1556},
  issn = {0265-4075},
  doi = {10/gjqwrm},
  abstract = {According to life history theory, exposure to harshness and/or unpredictability early in life should promote a fast life history strategy. Such a strategy entails, among other traits, elevated aggression and impaired relationship functioning. While detrimental under safe and stable conditions, these characteristics become more evolutionary adaptive in a harsh and/or ever-changing environment in which risks are uncertain and the future is difficult to predict. Hence, individuals who experienced harshness and/or unpredictability in their early home environment should grow up to have more conflictual relationships and be at greater risk for experiencing or perpetrating intimate partner violence (IPV). We tested this hypothesis on 179 participants in the Minnesota Longitudinal Study of Risk and Adaptation, an ongoing prospective longitudinal study that has followed individuals from before they were born into adulthood. IPV was assessed by the Conflict Tactics Scale at ages 23, 26, and 32. As expected, experiencing more unpredictability during the first 5 years of life (indexed by frequent changes in parents' employment status, cohabitation status, and residence) prospectively predicted both perpetrating and being the victim of IPV between ages 20 and 32. Experiencing harshness during the first 5 years of life (indexed by low socioeconomic status) only predicted being the victim of IPV. The early unpredictability effect on IPV perpetration was partially mediated by having more conflictual friendships during adolescence (assessed by a coder-rated friendship interview at age 16). These findings highlight the roles of early-life harshness and unpredictability both in promoting interpersonal conflict and violence and in impairing the capacity of individuals to maintain satisfying romantic relationships.},
  langid = {english},
  annotation = {WOS:000463622400009}
}

@article{szepsenwol2021,
  title = {The Effects of Childhood Unpredictability and Harshness on Emotional Control and Relationship Quality: {{A}} Life History Perspective},
  shorttitle = {The Effects of Childhood Unpredictability and Harshness on Emotional Control and Relationship Quality},
  author = {Szepsenwol, Ohad and Simpson, Jeffry A. and Griskevicius, Vladas and Zamir, Osnat and Young, Ethan S. and Shoshani, Anat and Doron, Guy},
  year = {2021},
  month = dec,
  journal = {Development and Psychopathology},
  pages = {1--14},
  publisher = {{Cambridge University Press}},
  issn = {0954-5794, 1469-2198},
  doi = {10.1017/s0954579421001371},
  urldate = {2022-02-23},
  abstract = {Being able to control oneself in emotionally upsetting situations is essential for good relationship functioning. According to life history theory, childhood exposure to harshness and unpredictability should forecast diminished emotional control and lower relationship quality. We examined this in three studies. In Studies 1 and 2, greater childhood unpredictability (frequent financial, residential, and familial changes), but not harshness (low SES), was associated with lower emotional control in adolescents (N = 1041) and adults (N = 327). These effects were stronger during the participants' reproductive years. Moreover, in Study 2, greater childhood unpredictability was indirectly associated with lower relationship quality through lower emotional control. In study 3, we leveraged the Minnesota Longitudinal Study of Risk and Adaptation (N = 160). Greater early-life unpredictability (ages 0\textendash 4) prospectively predicted lower relationship quality at age 32 via lower emotional control at the same age. This relation was serially mediated by less supportive observed early maternal care (ages 1.5\textendash 3.5) and insecure attachment representations (ages 19 and 26). Early unpredictability also predicted greater observed emotional distress during conflict interactions with romantic partners (ages 19\textendash 36). These findings point to the role of emotional control in mediating the effects of unpredictable childhood environments on relationship functioning in adulthood.},
  copyright = {All rights reserved},
  langid = {english},
  keywords = {attachment,childhood unpredictability,emotion regulation,life history theory,romantic relationships}
}

@article{szepsenwol2021a,
  title = {Identifying Developmental Adaptations to Early-life Stress},
  author = {Szepsenwol, Ohad},
  year = {2021},
  month = dec,
  journal = {Infant \& Child Development},
  pages = {1},
  issn = {15227227},
  doi = {10/gphn6f},
  urldate = {2022-02-16},
  abstract = {The adaptation-based approach to development under stress emphasizes how early-life environments calibrate the development of traits and capabilities in ways that increase evolutionary fitness. Rooted in evolutionary theory, the adaptation-based approach offers an alternative to the prevailing deficit model for development under stress. The utility and impact of this approach, however, depend on the ability to find convincing evidence for developmental adaptations to stress and eliminate alternative explanations. In this essay, I briefly discuss some of the challenges faced by the adaptation-based approach, specifically as it employs life history theory. I also suggest several avenues for improvement, including generating specific theoretical predictions, using consistent operational definitions, designing studies with adequate controls and assessing mediating pathways.}
}

@article{szollosi,
  title = {Arrested Theory Development: {{The}} Misguided Distinction between Exploratory and Confirmatory Research},
  shorttitle = {Arrested {{Theory Development}}},
  author = {Szollosi, Aba and Donkin, Chris},
  journal = {Perspectives on Psychological Science},
  pages = {1745691620966796},
  publisher = {{Sage Publications Ltd}},
  address = {{London}},
  issn = {1745-6916},
  doi = {10/gh59cr},
  abstract = {Science progresses by finding and correcting problems in theories. Good theories are those that help facilitate this process by being hard to vary: They explain what they are supposed to explain, they are consistent with other good theories, and they are not easily adaptable to explain anything. Here we argue that, rather than a lack of distinction between exploratory and confirmatory research, an abundance of flexible theories is a better explanation for the current replicability problems of psychology. We also explain why popular methods-oriented solutions fail to address the real problem of flexibility. Instead, we propose that a greater emphasis on theory criticism by argument might improve replicability.},
  langid = {english},
  annotation = {WOS:000620008500001}
}

