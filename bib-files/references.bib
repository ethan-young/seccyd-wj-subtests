@article{kirschbaum1994,
  title = {Salivary Cortisol in Psychoneuroendocrine Research: {{Recent}} Developments and Applications},
  shorttitle = {Salivary Cortisol in Psychoneuroendocrine Research},
  author = {Kirschbaum, Clemens and Hellhammer, Dirk H.},
  year = {1994},
